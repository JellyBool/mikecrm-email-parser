{
    "name": "jellybool/mikecrm-email-parser",
    "description": "Mikecrm Email Parser",
    "keywords": [
        "jellybool",
        "mikecrm-email-parser"
    ],
    "homepage": "https://github.com/jellybool/mikecrm-email-parser",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "JellyBool",
            "email": "ly913417651@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Jellybool\\MikeCRMEmailParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jellybool\\MikeCRMEmailParser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
